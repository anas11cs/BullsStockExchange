@{
    if (Session["compname"] == null)
    {
        Response.Redirect("~/db/resultpage");
    }
}
<!DOCtype html>
@using dbconnectivity_C.Models;
@model string
<html>
<head style="background-color:white">
    <title>Bull's Stock Exchange Pakistan</title>
    <style>
                ul {
                    list-style-type: none;
                    margin: 0;
                    padding: 0;
                    overflow: hidden;
                    border: 1px solid #e7e7e7;
                    background-color: white;
                }

                li {
                    float: left;
                }

                    li a {
                        display: block;
                        color: #666;
                        text-align: center;
                        padding: 14px 16px;
                        text-decoration: none;
                    }

                        li a:hover:not(.active) {
                            background-color: #ddd;
                        }

                .head1 {
                    font-size: 50px;
                    font-weight: bolder;
                    margin-left: 400px;
                    font-family: 'Agency FB';
                    margin-bottom: 1px;
                    color: white;
                    /* THE CHANGE TO ADD IS BELOW*/
                    text-shadow: 1px 2px 1px 2px #999;
                }

                .head2 {
                    font-size: 12px;
                    margin-left: 550px;
                    margin-bottom: 8px;
                    font-weight: bolder;
                    color: white
                }

                .head3 {
                    font-size: 10px;
                    margin-left: 440px;
                    margin-bottom: 20px;
                    font-weight: bolder;
                    color: white
                }

                .body_sec {
                    margin-left: 480px;
                }

                .body_sec1 {
                    margin-left: 10px;
                    margin-bottom: 15px;
                }

                .body_sec2 {
                    margin-bottom: 20px;
                    margin-left: 10px;
                }

                .body_sec3 {
                    margin-left: 485px;
                }

                .foot {
                    margin-top: 20px
                }

                .button:active {
                    background-color: black;
                    box-shadow: 0 5px #272a3c;
                    transform: translateY(4px);
                }

                .button:hover {
                    background-color: white;
                }

                .button {
                    padding: 2.5px 10px;
                    font-size: 12px;
                    font-family: Bebas;
                    text-align: center;
                    cursor: pointer;
                    outline: none;
                    color: black;
                    background-color: firebrick;
                    border: none;
                    border-radius: 10px;
                    box-shadow: 0 4px #999;
                    margin-left: 535px;
                }

                .jangli {
                    display: block;
                    color: #666;
                    text-align: center;
                    padding: 14px 16px;
                    text-decoration: none;
                }

                .head4 {
                    /*padding:10px 5px 15px 20px;
        top padding is 10px
        right padding is 5px
        bottom padding is 15px
        left padding is 20px*/
                    font-size: 100px;
                    font-weight: bolder;
                    font-family: 'Script MT';
                    color: white;
                    border-radius: 60px;
                    text-shadow: 3px 3px 3px 3px #656565;
                    margin-bottom: 15px;
                    margin-left: 260px;
                }

                    .head4:hover {
                        text-shadow: 2.5px 2.5px 2.5px 2.5px #a4fff7;
                        color: black;
                    }
        .align {
            align-items: center;
            display: flex;
            flex-direction: column;
            justify-content: center;
            color: black;
            font-size: 100px;
            font-weight: 900;
            font-family: Asap;
        }
        .align:hover{
            font-size:120px;
            text-shadow:1px 1px 1px 1px #ffffff;
        }
        .bik {
            margin-top: 25px;
            width: 400px;
            height: 70px;
            -webkit-border-radius: 8px/7px;
            -moz-border-radius: 8px/7px;
            border-radius: 8px/7px;
            background-color: silver;
            margin-left: 385px;
            -webkit-box-shadow: 1px 2px 5px rgba(0,0,0,.31);
            -moz-box-shadow: 1px 2px 5px rgba(0,0,0,.31);
            box-shadow: 0 6px rgb(255, 255, 255);
            /*padding:10px 5px 15px 20px;
        top padding is 10px
        right padding is 5px
        bottom padding is 15px
        left padding is 20px*/
            border: solid 1px #cbc9c9;
            font-size: 35px;
            font-weight: bolder;
            font-family: 'Script MT';
            color: black;
            border-radius: 60px;
        }
    </style>
</head>
<body style="background:black">
    <header>
        <a class="head1" href="../../db/Index">Bull's Stock Exchange</a>
        <div class="head2"> PAKISTAN</div>
        <div class="head3">Intelligent - Stock Broking & Buying Platform for Investors</div>
        <ul>
            <li><a href="../../db/logout">Home</a></li>
            <li><a href="../../db/myshareholders">Share Holders</a></li>
            <li><a href="../../db/brokeipos">Broke Ipos</a></li>
            <li style="float:right"><a href="../../db/logoutcompany">Logout</a></li>
            @{
                if (Session["compname"] != null)
                {
                    <li style="float:right" class="jangli">Company Logged In:  @Session["compname"].ToString()</li>
                }
            }

        </ul>
    </header>
    <br />
    <br />
    <br />
    <br />
    <div><p class="head4">Welcome Bull !</p></div>
    <br />
    <div class="adj"><label style="color:red" ;>@Model</label></div>
    <hr />
</body>
</html>